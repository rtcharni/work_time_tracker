{
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "ESNext",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": false,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        },
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./src/**/*",
        "utils/constants.ts"
    ],
}